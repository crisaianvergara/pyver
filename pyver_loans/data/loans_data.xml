<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <!-- Loan Request Sequence: Loan Number-->
        <record id="loan_request_data_squence" model="ir.sequence">
            <field name="name">loan.request.data.squence</field>
            <field name="code">loan.request</field>
            <field name="prefix">LOAN</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Loan Type Sequence: Code -->
        <record id="loan_type_data_squence" model="ir.sequence">
            <field name="name">loan.type.data.squence</field>
            <field name="code">loan.type</field>
            <field name="prefix">LT</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>